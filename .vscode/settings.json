{
    "files.associations": {
        "*.xml": "xml"
    },
    "editor.unusualLineTerminators": "ignore",
    "[xml]": {
        "editor.formatOnSave": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 120
    },
    "files.autoGuessEncoding": true
}